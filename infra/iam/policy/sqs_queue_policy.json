{
  "Version": "2012-10-17",
  "Statement": [
    {
      "Effect": "Allow",
      "Principal": {
        "AWS": "${ROOT_USER}"
      },
      "Action": "SQS:*",
      "Resource": "${QUEUE_ARN}"
    },
    {
      "Effect": "Allow",
      "Principal": {
        "AWS": "*"
      },
      "Action": "SQS:SendMessage",
      "Resource": "${QUEUE_ARN}"
    }
  ]
}